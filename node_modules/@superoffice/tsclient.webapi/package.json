{
  "name": "@superoffice/tsclient.webapi",
  "version": "10.2.6-1262",
  "private": false,
  "description": "TypeScript client for SuperOffice WebApi.",
  "main": "dist/WebApi.js",
  "module": "dist-esm/WebApi.js",
  "types": "dist/WebApi.d.ts",
  "source": "src/WebApi.ts",
  "sideEffects": false,
  "scripts": {
    "tsc": "tsc",
    "build": "tsc && tsc --build tsconfig.esm.json",
    "build:prod": "tsc",
    "build:esm": "tsc --build tsconfig.esm.json",
    "watch": "tsc --build tsconfig.esm.json --watch",
    "pretest": "rimraf TEST-*.xml",
    "test": "jest --all --ci --no-cache --watchAll=false",
    "test-ci": "jest --all --ci --no-cache --watchAll=false --maxWorkers=2",
    "testRunInBand": "jest --all --ci --runInBand --no-cache --watchAll=false",
    "clean": "rimraf dist dist-esm",
    "lint": "eslint -c ../../.eslintrc.json **/*.ts"
  },
  "author": "SuperOffice AS",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.7",
    "jest": "^29.5.0",
    "jest-junit": "^13.2.0",
    "jest-junit-reporter": "^1.1.0",
    "nock": "^13.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "moment": "^2.29.4",
    "tslib": "^2.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "globals": {},
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "./tsconfig.jest.json"
        }
      ]
    },
    "testMatch": [
      "<rootDir>/__tests__/**/*Test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": false,
    "preset": "ts-jest"
  },
  "jest-junit": {
    "suiteName": "TSClient WebAPI tests",
    "output": "./TEST-TSWebAPI.xml"
  }
}
